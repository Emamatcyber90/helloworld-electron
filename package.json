{
  "name":    "nanodemo",
  "version": "0.0.0",
  "license": "MIT",
  "main":    "main.js",
  "description": "nanodemo",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/visorgg/potato.git"
  },
  "build": {
    "productName": "nanodemo",
    "appId": "com.nanodemo",
    "win": {
      "publisherName": "nanodemo",
      "publish": {
        "provider": "s3",
        "bucket": "nanodemo"
      },
      "target": "nsis"
    },
    "asar": true,
    "nsis": {
      "oneClick": true,
      "allowToChangeInstallationDirectory": false,
      "perMachine": true,
      "artifactName": "${productName} Installer ${version}.${ext}"
    }
  },
  "scripts": {
    "dev": "electron .",
    "build": "electron-builder"
  },
  "devDependencies": {
    "electron": "3.1.3",
    "electron-builder": "^20.11.1"
  }
}
